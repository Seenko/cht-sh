{
  "name": "cht-sh",
  "version": "1.0.8",
  "type": "module",
  "description": "A Node cheat.sh CLI",
  "repository": "Seenko/cht-sh",
  "main": "/dist/cht.sh.js",
  "files": [
    "dist/**/*"
  ],
  "bin": "./dist/cht.sh.js",
  "scripts": {
    "dev": "npm run build -- -w",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "build": "npm run lint && tsc -p .",
    "clean": "rimraf dist",
    "prepublish": "npm run clean && npm run build"
  },
  "keywords": [
    "cli",
    "cheat.sh",
    "cht.sh",
    "cheatsheet"
  ],
  "author": "Seenko",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "node-fetch": "^3.2.4"
  }
}
